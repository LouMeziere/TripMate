  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "ml-kmeans": "^6.0.0",
    "node-kmeans": "^1.1.9",
    "openai": "^5.10.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
